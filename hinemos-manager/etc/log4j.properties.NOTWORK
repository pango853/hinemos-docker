log4j.rootCategory=INFO, console
log4j.additivity.HinemosInternal=false

log4j.category.com.clustercontrol.HinemosManagerMain=INFO, console
log4j.category.HinemosOperation=INFO, console
log4j.category.HinemosInternal=INFO, console
